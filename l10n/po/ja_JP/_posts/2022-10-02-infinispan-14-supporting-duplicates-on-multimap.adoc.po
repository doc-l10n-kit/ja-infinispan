#. type: Title =
#: upstream/_posts/2022-10-02-infinispan-14-supporting-duplicates-on-multimap.adoc:10
#, no-wrap
msgid "Infinispan 14 supporting duplicates on multimap"
msgstr ""

#. type: Plain text
#: upstream/_posts/2022-10-02-infinispan-14-supporting-duplicates-on-multimap.adoc:13
msgid "Dear Infinispan community,"
msgstr ""

#. type: Plain text
#: upstream/_posts/2022-10-02-infinispan-14-supporting-duplicates-on-multimap.adoc:15
msgid "With the Infinispan 14 development release 04 multimap cache supports duplicates."
msgstr ""

#. type: Plain text
#: upstream/_posts/2022-10-02-infinispan-14-supporting-duplicates-on-multimap.adoc:17
msgid "By default `supportsDuplicates` is set to false and can be configured during initialization."
msgstr ""

#. type: Plain text
#: upstream/_posts/2022-10-02-infinispan-14-supporting-duplicates-on-multimap.adoc:19
msgid "The following is an example on how you can set multimap to support duplicate key-value pairs:"
msgstr ""

#. type: delimited block -
#: upstream/_posts/2022-10-02-infinispan-14-supporting-duplicates-on-multimap.adoc:23
#, no-wrap
msgid "public class MyEmbeddedMultimapCacheExmaple {\n"
msgstr ""

#. type: delimited block -
#: upstream/_posts/2022-10-02-infinispan-14-supporting-duplicates-on-multimap.adoc:26
#, no-wrap
msgid ""
"public static void main(String[] args) {\n"
"      DefaultCacheManager cacheManager = new DefaultCacheManager();\n"
msgstr ""

#. type: delimited block -
#: upstream/_posts/2022-10-02-infinispan-14-supporting-duplicates-on-multimap.adoc:28
#, no-wrap
msgid "      MultimapCacheManager multimapCacheManager = EmbeddedMultimapCacheManagerFactory.from(cacheManager);\n"
msgstr ""

#. type: delimited block -
#: upstream/_posts/2022-10-02-infinispan-14-supporting-duplicates-on-multimap.adoc:30
#, no-wrap
msgid "      multimapCacheManager.defineConfiguration(\"multimap\", new ConfigurationBuilder().build());\n"
msgstr ""

#. type: delimited block -
#: upstream/_posts/2022-10-02-infinispan-14-supporting-duplicates-on-multimap.adoc:33
#, no-wrap
msgid ""
"      // Get the MultimapCache with duplicates support\n"
"      MultimapCache<String, String> multimap = multimapCacheManager.get(\"multimap\", true);\n"
msgstr ""

#. type: delimited block -
#: upstream/_posts/2022-10-02-infinispan-14-supporting-duplicates-on-multimap.adoc:44
#, no-wrap
msgid ""
"      // Store duplicate values in a key\n"
"      CompletableFuture.allOf(\n"
"            multimap.put(\"key\", \"value1\"),\n"
"            multimap.put(\"key\", \"value1\"),\n"
"            multimap.put(\"key\", \"value3\"))\n"
"            .whenComplete((nil, ex) -> {\n"
"               // Retrieve the values\n"
"               multimap.get(\"key\").whenComplete((values, ex2) -> {\n"
"                  // Print them out\n"
"                  System.out.println(values);\n"
msgstr ""

#. type: delimited block -
#: upstream/_posts/2022-10-02-infinispan-14-supporting-duplicates-on-multimap.adoc:52
#, no-wrap
msgid ""
"                  System.out.println(\"Multimap size:\" + values.size());\n"
"                  // Stop the cache manager and release all resources\n"
"                  cacheManager.stop();\n"
"               });\n"
"            });\n"
"   }\n"
"}\n"
msgstr ""

#. type: delimited block -
#: upstream/_posts/2022-10-02-infinispan-14-supporting-duplicates-on-multimap.adoc:58
#, no-wrap
msgid ""
"[value1, value1, value3]\n"
"Multimap size: 3\n"
msgstr ""

